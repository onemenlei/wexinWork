page{color:#787878;}
swiper-item image{width:100%;height:350rpx;}
.title-view{background:#f0f0f0;}
.title-text{display: flex;justify-content:space-between;align-items:center;padding:30rpx 20rpx;flex-flow:row;}
.title-text view{display: flex;align-items:center;}
.title-text view text{margin-left:15rpx;}
.title-text-image{width:80rpx;height: 80rpx;border-radius: 8rpx;}
.title-text>text text{color:#000}
.inam-image{width:45rpx;height:45rpx;}
.inam-text{font-size: 30rpx;}
.mmok-title{padding:10rpx 20rpx;font-size:35rpx;}
.mmok-pzbz{display: flex;flex-flow:row;justify-content:space-around;padding:20rpx 0;}
.mmok-pzbz>view{display: flex;flex-flow:column;align-items: center;}
.mmok-pzbz>view image{width:70rpx;height:70rpx;}
.mmok-pzbz>view text{font-size: 26rpx;}
.line-image{width:100%;height:160rpx}
.title-menu-text{display: flex;justify-content: center;font-size: 50rpx;align-items: center;border-bottom:1px dashed #f0f0f0;padding-bottom:10rpx;margin-bottom:10rpx;}
.title-menu-text>image{width:50rpx;height:50rpx;padding-right:20rpx;}
.image-menu-view{display: flex;padding:20rpx;justify-content: center;}
.image-menu-view image{width:100%;border-radius: 20rpx;}
.image-menu-view navigator{position:relative;}
.image-menu-view-title{display: flex;justify-content: space-between;}
.image-menu-view-title>view:first-child{width:80%;}
.image-menu-view-title>view:last-child{width:20%;}
.image-menu-view-text{color:#000;width:100%;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 1;-webkit-box-orient: vertical;}
.image-menu-view-Subtitle{color:#a0a0a0;width:100%;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 1;-webkit-box-orient: vertical;font-size: 34rpx;margin-top:20rpx;}
.image-menu-view-price{width:100%;overflow: hidden;font-size: 26rpx;} 
.image-menu-view-button{width:100%;border-radius: 60rpx;background:#ff4479;color:#fff;text-align: center;font-size: 21rpx;margin-top:20rpx;}
.image-menu-view-button.gray{background:#a0a0a0;}
.mask{background:#ff4479;width:200rpx;height: 70rpx;line-height:70rpx;text-align:center;border-radius: 20rpx 0;position: absolute;top:0;left:0;color:#fff;font-size: 26rpx;}
.mask b{position: absolute;top:-20rpx;width:16rpx;height: 30rpx;background:#ff4479;border:5rpx solid #fff;z-index: 2;border-radius:20rpx;}
.mask b:first-child{left:20rpx;}
.mask b:nth-child(2){left:50rpx;}
.mask b:nth-child(3){left:130rpx;}
.mask b:nth-child(4){left:160rpx;}
.image-nobus{width:80%;margin:0 10%;height:460rpx;}