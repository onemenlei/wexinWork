.order-object-main{display: flex;justify-content: space-between;padding:20rpx;align-items:flex-start;border-bottom:10rpx solid #fff;background: #f3f3f3;}
.order-object-main icon{align-self: center;}
image.imgsize{width:200rpx;height: 200rpx;margin:0 10rpx;display: block;}
.order-object-main>view{width:75%;font-size: 28rpx;}
.order-object-main-price{display: flex;justify-content: space-between;}
.order-object-main-color{color:#a0a0a0;font-size:26rpx;}
.order-object-main-title{overflow: hidden;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}
.order-object-sum{background:#fff;padding:20rpx;display: flex;flex-flow:column;justify-content: space-between;}
.order-object-sum>view:first-child{width:100%;display: flex;justify-content: space-between;}
.order-object-sum>view:last-child{padding-top:15rpx;text-align: right;}
.order-object-sum>view:last-child button{font-size:28rpx;display:inline-block;}
.num-button{padding:10rpx 0;height: 50rpx;}
.num-button text{width:40rpx;height:100%;line-height:50rpx;text-align:center;display:block;border:1px solid #333;float:left;box-sizing:border-box;}
.num-button text:nth-child(2){border-left-width: 0;border-right-width: 0;background:#fff;}
.bottom-view{background:#a0a0a0;font-size:30rpx;color:#fff;width:100%;position:fixed;bottom:0rpx;}
.bottom-view>view{display: flex;align-items: center;justify-content: space-between;padding:20rpx 10rpx;}
.iconself{align-self:center;vertical-align:middle;padding:0 10rpx; }